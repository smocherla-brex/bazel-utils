load("@io_bazel_stardoc//stardoc:stardoc.bzl", "stardoc")
load("@bazel_skylib//:bzl_library.bzl", "bzl_library")

bzl_library(
    name = "bazel_tools_srcs",
    srcs = [
        "@bazel_tools//tools:bzl_srcs",
    ],
)

stardoc(
    name = "github-docs",
    out = "github_doc.md",
    input = "github.bzl",
    deps = [":bazel_tools_srcs"],
)
